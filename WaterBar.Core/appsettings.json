{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StatusBarOption": {
    "Interval": 1,
    "Display": [
      {
        "Target": "Network",
        "Select": "enp34s0",
        "Format": "\uDB81\uDD52 up_speed, \uDB80\uDDDA down_speed \uDB81\uDCC5 bandwidth"
      },
      {
        "Target": "Datetime",
        "Format": "\uDB82\uDD54 yyyy-mm-dd HH:MM:ss"
      }
    ]
  }
}
