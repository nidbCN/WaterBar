{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StatusBarOption": {
    "Interval": 1,
    "Display": [
      {
        "Target": "Network",
        "Select": "enp34s0",
        "Format": "\uDB81\uDD52 %up_speed, \uDB80\uDDDA %down_speed %speed"
      }
    ]
  }
}
