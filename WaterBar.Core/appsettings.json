{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StatusBarOption": {
    "Interval": 1,
    "Display": [
      {
        "Target": "Keyboard",
        "Format": "\udb82\ude9b caps_lock"
      },
      {
        "Target": "Network",
        "Select": "wlp38s0",
        "Format": "Wifi: \uDB81\uDD52 up_speed, \uDB80\uDDDA down_speed"
      },
      {
        "Target": "Network",
        "Select": "enp34s0",
        "Format": "Eth: \uDB81\uDD52 up_speed, \uDB80\uDDDA down_speed  \uDB81\uDCC5 bandwidth"
      },
      {
        "Target": "Datetime",
        "Format": "\uDB82\uDD54 yyyy-MM-dd HH:mm:ss"
      },
      {
        "Target": "Processor",
        "Format": "total_usage",
        "Color": "blue"
      }
    ]
  }
}
